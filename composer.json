{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for ZF2",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "repositories": {
        "doctrine2-nestedset": {
            "type": "package",
            "package": {
                "name": "doctrine2-extensions/doctrine2-nestedset",
                "version": "dev-master",
                "source": {
                    "url": "git://github.com/blt04/doctrine2-nestedset.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    },
    "minimum-stability": "alpha",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": ">2.1.3",
        "zendframework/zend-developer-tools": "dev-master",
        "doctrine/doctrine-orm-module": "0.*",
        "doctrine/data-fixtures": "dev-master",
        "doctrine2-extensions/doctrine2-nestedset": "dev-master",
        "dl-commons/dlc-base": "dev-master",
        "dl-commons/dlc-doctrine": "dev-master",
        "dl-commons/dlc-category": "dev-master",
        "dl-commons/dlc-diagramm": "dev-master",
        "dl-commons/dlc-use-case": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "DoctrineExtensions\\NestedSet": "vendor/doctrine2-nestedset/lib"
        }
    }
}
